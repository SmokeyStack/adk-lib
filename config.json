{
	"$schema": "https://raw.githubusercontent.com/Bedrock-OSS/regolith-schemas/main/config/v1.1.json",
	"author": "SmokeyStack",
	"name": "adk-lib",
	"packs": {
		"behaviorPack": "./packs/BP",
		"resourcePack": "./packs/RP"
	},
	"regolith": {
		"dataPath": "./packs/data",
		"filterDefinitions": {
			"move-manifest": {
				"script": "./filters/move-manifest.py",
				"runWith": "python"
			}
		},
		"profiles": {
			"dev": {
				"export": {
					"readOnly": false,
					"target": "preview"
				},
				"filters": [
					{
						"arguments": [
							"manifest.beta.json"
						],
						"filter": "move-manifest"
					}
				]
			},
			"build": {
				"export": {
					"readOnly": false,
					"target": "local"
				},
				"filters": []
			}
		}
	}
}